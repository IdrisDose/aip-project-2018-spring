{
  "name": "Order",
  "plural": "orders",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "qty": {
      "type": "number",
      "required": true
    },
    "isCompleted": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "time": {
      "type": "date",
      "required": true,
	  "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": ""
    },
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    },
    "products": {
      "type": "embedsOne",
      "model": "Product",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
